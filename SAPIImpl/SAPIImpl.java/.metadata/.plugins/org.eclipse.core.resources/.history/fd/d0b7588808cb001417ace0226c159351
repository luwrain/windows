package org.luwrain.windows.speech;

class  SAPIImpl
{
	static
	{
		// загружаем динамическу библиатеку под текущую архитектуру(32 или 64 бита)
    	System.loadLibrary("SAPIImpl."+System.getProperty("sun.arch.data.model"));
	}
	native public static void Say(String text);
	
	
}